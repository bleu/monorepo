{
  "name": "@bleu/balancer-tools",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "pnpm run build:prepare:gql && next build",
    "build:prepare:gql": "cd ../../ && pnpm run -C=packages/gql graphql:update-types",
    "build:prepare:wagmi": "pnpm run wagmi",
    "build:prepare": "pnpm npm-run-all -l --parallel 'build:prepare:gql'",
    "dev": "pnpm npm-run-all -l --parallel dev:**",
    "dev:wagmi": "wagmi generate --watch",
    "dev:next": "next dev",
    "start": "next start",
    "wagmi": "wagmi generate",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "eslint '**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@balancer-labs/sor": "4.1.1-beta.13",
    "@bleu-balancer-tools/gql": "workspace:*",
    "@bleu-balancer-tools/math-poolsimulator": "workspace:*",
    "@bleu-balancer-tools/math-stableswapsimulator": "workspace:*",
    "@bleu-balancer-tools/tsconfig": "workspace:*",
    "@hookform/resolvers": "3.1.0",
    "@radix-ui/colors": "^1.0.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-select": "1.2.2",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.4",
    "@radix-ui/react-tooltip": "1.0.6",
    "@rainbow-me/rainbowkit": "1.0.6",
    "@sentry/nextjs": "^7.59.2",
    "@wagmi/cli": "1.3.0",
    "@wagmi/core": "^1.3.8",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "copy-to-clipboard": "^3.3.3",
    "graphql-request": "5.1.0",
    "inputmask": "^5.0.8",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "next": "^13.4.10",
    "plotly.js": "^2.24.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.43.9",
    "react-plotly.js": "^2.6.0",
    "swr": "^2.2.0",
    "tailwind-merge": "^1.14.0",
    "viem": "^1.3.0",
    "wagmi": "^1.3.9",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@bleu-balancer-tools/eslint-config": "workspace:^",
    "@bleu-balancer-tools/math-stableswapsimulator": "workspace:^",
    "@bleu-balancer-tools/utils": "workspace:^",
    "@ethersproject/bignumber": "^5.7.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "14.4.3",
