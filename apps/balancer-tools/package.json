    "@types/testing-library__jest-dom": "5.14.8",
    "autoprefixer": "^10.4.14",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.26",
    "tailwind-scrollbar": "^3.0.4",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "5.1.6"
  }
