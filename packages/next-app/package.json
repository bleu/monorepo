{
  "name": "@balancer-pool-metadata/next-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "pnpm run build:prepare:gql && next build",
    "build:prepare:gql": "cd ../../ && pnpm run -C=packages/gql graphql:update-types",
    "build:prepare:wagmi": "pnpm run wagmi",
    "build:prepare": "pnpm npm-run-all -l --parallel 'build:prepare:gql'",
    "dev": "pnpm npm-run-all -l --parallel dev:**",
    "dev:wagmi": "wagmi generate --watch",
    "dev:next": "next dev",
    "start": "next start",
    "wagmi": "wagmi generate",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint . --fix --resolve-plugins-relative-to ."
  },
  "dependencies": {
    "@balancer-pool-metadata/gql": "workspace:*",
    "@balancer-pool-metadata/schema": "workspace:*",
    "@balancer-pool-metadata/tsconfig": "workspace:*",
    "@hookform/resolvers": "^3.0.1",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-alert-dialog": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-progress": "^1.0.2",
    "@radix-ui/react-select": "1.2.1",
    "@radix-ui/react-toast": "^1.1.3",
    "@radix-ui/react-tooltip": "1.0.5",
    "@rainbow-me/rainbowkit": "0.12.7",
    "@wagmi/cli": "0.1.15",
    "@wagmi/core": "^0.10.9",
    "@xstate/react": "^3.2.1",
    "classnames": "^2.3.2",
    "ethers": "^5.7.2",
    "graphql-request": "5.1.0",
    "lodash": "^4.17.21",
    "lodash.merge": "^4.6.2",
    "next": "^13.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "swr": "^2.1.2",
    "wagmi": "0.12.10",
    "xstate": "^4.37.1"
  },
  "devDependencies": {
    "@balancer-pool-metadata/shared": "workspace:^",
    "@types/lodash": "^4.14.192",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.3.1",
    "typescript": "5.0.3"
  }
}
